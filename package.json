{
  "name": "webpack-frontend-backend-boilerplate",
  "version": "0.0.1",
  "description": "This boilerplate will provide you a ready to develop Angular frontend and an ExpressJS based on TypeScript and compiled with WebPack",
  "main": "build/server.js",
  "scripts": {
    "test": "jest --watch",
    "build:prod": "webpack --mode production --env.production",
    "build:dev": "webpack --mode development --env.development",
    "watch": "webpack --mode development --watch --env.development --env.watch --env.inspect",
    "start": "node build/server.js",
    "ndb:watch": "ndb webpack --mode development --watch --env.development --env.watch",
    "ndb:start": "ndb node build/server.js --env.noinspect"
  },
  "author": "Florian Kapust (ovi1337) - src-one.com",
  "license": "MIT",
